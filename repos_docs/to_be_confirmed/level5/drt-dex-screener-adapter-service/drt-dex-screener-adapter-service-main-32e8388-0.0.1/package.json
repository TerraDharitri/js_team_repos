    "start:api:mainnet": "cp ./.env.mainnet ./.env & npm run copy-api-config & nest build api & TZ=UTC nest start --watch --debug",
    "start:api:custom": "cp ./.env.custom ./.env & npm run copy-api-config & nest build api & TZ=UTC nest start --watch --debug",
    "start:offline-jobs:mainnet": "cp ./.env.mainnet ./.env & npm run copy-offline-jobs-config & nest build offline-jobs & TZ=UTC nest start offline-jobs --watch --debug",
