{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@drt-monorepo/common(|/.*)$": "<rootDir>/../../../libs/common/src/$1",
    "^@drt-monorepo/common": "<rootDir>/../../../libs/common"
  }
}